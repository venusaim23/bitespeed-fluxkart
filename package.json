{
  "name": "bitespeed-fluxkart",
  "version": "1.0.0",
  "description": "Identify customers across multiple purchases with common mobile and email",
  "main": "app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon dist/app.js",
    "build": "tsc",
    "seed_database": "knex seed:run",
    "create_seed": "knex seed:make --migrations-directory src/models/seeds",
    "run_next_migration": "knex migrate:up --migrations-directory src/models/migrations",
    "undo_last_migration": "knex migrate:down --migrations-directory src/models/migrations",
    "create_migration": "knex migrate:make --migrations-directory src/models/migrations"
  },
  "keywords": [],
  "author": "Venu Sai Madisetti",
  "license": "LGPL-3.0-only",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.0",
    "@types/http-errors": "^2.0.4",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.1.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
